{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/test-gemini",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 